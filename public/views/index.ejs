<!DOCTYPE html>
<html lang="pl">
    <head>
        <meta charset="utf-8">

        <title>Index</title>

        <link href="css/vendor/bootstrap/bootstrap.css" rel="stylesheet">
        <link href="css/vendor/font-awesome/font-awesome.css" rel="stylesheet">
        <link href="css/vendor/nprogress/nprogress.css" rel="stylesheet">
        <link href="css/vendor/dropzone/dropzone.css" rel="stylesheet">
        <link href="css/vendor/custom/custom.css" rel="stylesheet">
    </head>

    <body class="nav-md">
        <div class="container body">
            <div class="main_container">
                <div class="col-md-3 left_col">
                    <div class="left_col scroll-view">
                        <div class="navbar nav_title" style="border: 0;">
                            <a href="/" class="site_title"><i class="fa fa-paw"></i> <span>Print Photo</span></a>
                        </div>

                        <div class="clearfix"></div>

                        <div class="profile">
                            <div class="profile_pic">
                                <img src="images/img.jpg" alt="..." class="img-circle profile_img">
                            </div>
                            <div class="profile_info">
                                <span>Welcome</span>
                                <h2> <%= user.local.firstName %></h2>
                            </div>
                        </div>

                        <br />

                        <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
                            <div class="menu_section">
                                <h3>General</h3>
                                <ul class="nav side-menu">
                                    <li><a href="/"><i class="fa fa-home"></i> Home</a>
                                    </li>
                                </ul>
                                <ul class="nav side-menu">
                                    <li><a><i class="fa fa-picture-o"></i> Gallery <span class="fa fa-chevron-down"></span></a>
                                        <ul class="nav child_menu">
                                            <li><a href="/gallery/photos">Show Photos</a></li>
                                            <li><a href="/gallery/albums">Show Albums</a></li>
                                            <li><a href="/gallery/photo/add">Add Photos</a></li>
                                        </ul>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="top_nav">
                    <div class="nav_menu">
                        <nav>
                            <div class="nav toggle">
                                <a id="menu_toggle"><i class="fa fa-bars"></i></a>
                            </div>

                            <ul class="nav navbar-nav navbar-right">
                                <li class="">
                                    <a href class="user-profile dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                        <img src="images/img.jpg" alt="">John Doe
                                        <span class=" fa fa-angle-down"></span>
                                    </a>
                                    <ul class="dropdown-menu dropdown-usermenu pull-right">
                                        <li><a href="/profile"> Profile</a></li>
                                        <li><a href="/logout"><i class="fa fa-sign-out pull-right"></i> Log Out</a></li>
                                    </ul>
                                </li>
                            </ul>
                        </nav>
                    </div>
                </div>

                <div class="right_col" role="main">
                    <div class="row title_count">
                        <div class="page-title">
                            <div class="title_left">
                                <h3>Main Page</h3>
                            </div>
                        </div>

                        <div class="clearfix"></div>

                        <div class="row">
                            <% for(var i = 0; i < 9; i++) { %>
                                <div class="col-lg-4 col-md-6 col-sm-12 col-xs-12">
                                    <div class="panel panel-default">
                                        <div class="panel-heading">
                                            <h2>Title<%= (i + 1) %></h2>
                                        </div>
                                        <div class="panel-body">
                                            BODY
                                        </div>
                                    </div>
                                </div>
                            <% } %>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <script src="js/vendor/jquery/jquery.js"></script>
        <script src="js/vendor/bootstrap/bootstrap.js"></script>
        <script src="js/vendor/fastclick/fastclick.js"></script>
        <script src="js/vendor/nprogress/nprogress.js"></script>
        <script src="js/vendor/dropzone/dropzone.min.js"></script>
        <script src="js/vendor/custom/custom.js"></script>
    </body>
</html>
